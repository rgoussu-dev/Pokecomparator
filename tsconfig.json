{
  "compileOnSave": false,
  "compilerOptions": {
    "paths": {
      "@domain/": [
        "./projects/domain/*",
        "./dist/domain"
      ],
      "@infra/*": [
        "./projects/infra/*",
        "./dist/infra"
      ],
      "@ui": [
        "./projects/ui/src/public-api.ts",
        "./dist/ui"
      ],
      "ui": [
        "./dist/ui"
      ]
    },
    "strict": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "ES2022",
    "module": "preserve"
  },
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "strictTemplates": true
  },
  "files": [],
  "references": [
    {
      "path": "./projects/host/tsconfig.app.json"
    },
    {
      "path": "./projects/host/tsconfig.spec.json"
    },
    {
      "path": "./projects/remote-catalog/tsconfig.app.json"
    },
    {
      "path": "./projects/remote-catalog/tsconfig.spec.json"
    },
    {
      "path": "./projects/remote-detail/tsconfig.app.json"
    },
    {
      "path": "./projects/remote-detail/tsconfig.spec.json"
    },
    {
      "path": "./projects/remote-compare/tsconfig.app.json"
    },
    {
      "path": "./projects/remote-compare/tsconfig.spec.json"
    },
    {
      "path": "./projects/domain/tsconfig.lib.json"
    },
    {
      "path": "./projects/domain/tsconfig.spec.json"
    },
    {
      "path": "./projects/infra/tsconfig.lib.json"
    },
    {
      "path": "./projects/infra/tsconfig.spec.json"
    },
    {
      "path": "./projects/ui/tsconfig.lib.json"
    },
    {
      "path": "./projects/ui/tsconfig.spec.json"
    },
    {
      "path": "./projects/ui/tsconfig.lib.json"
    },
    {
      "path": "./projects/ui/tsconfig.spec.json"
    }
  ],
  "shared-lib":[
    "projects/ui/src/public-api.ts",
    "projects/domain/src/public-api.ts",
    "projects/infra/src/public-api.ts"
  ]
}
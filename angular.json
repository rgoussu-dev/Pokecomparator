{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "cli": {
    "packageManager": "npm"
  },
  "newProjectRoot": "projects",
    "schematics": {
        "@schematics/angular:component": {
          "style": "css"
        }
      },
  "projects": {
    "host": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "css"
        }
      },
      "root": "projects/host",
      "sourceRoot": "projects/host/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "ngx-build-plus:browser",
          "options": {
            "outputPath": "dist/host",
            "tsConfig": "projects/host/tsconfig.app.json",
            "inlineStyleLanguage": "css",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/host/public",
                "output": "."
              },
              {
                "glob": "**/*",
                "input": "./projects/ui/assets",
                "output": "assets/ui"
              }
            ],
            "styles": [
              "projects/host/src/styles.css"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "projects/ui/src/lib"
              ]
            },
            "main": "projects/host/src/main.ts",
            "index": "projects/host/src/index.html",
            "extraWebpackConfig": "projects/host/webpack.config.js",
            "commonChunk": false
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all",
              "extraWebpackConfig": "projects/host/webpack.prod.config.js"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "ngx-build-plus:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "host:build:production",
              "extraWebpackConfig": "projects/host/webpack.prod.config.js"
            },
            "development": {
              "buildTarget": "host:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "port": 4200,
            "publicHost": "http://localhost:4200",
            "extraWebpackConfig": "projects/host/webpack.config.js"
          }
        },
        "test": {
          "builder": "@angular/build:unit-test"
        }
      }
    },

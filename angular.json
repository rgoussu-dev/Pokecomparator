        "storybook": {
          "builder": "@storybook/angular:start-storybook",
          "options": {
            "configDir": "projects/ui/.storybook",
            "browserTarget": "ui:build",
            "compodoc": false,
            "port": 6006,
            "styles": [
              "projects/ui/src/lib/global-styles.css"
            ],
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/ui/assets",
                "output": "assets"
              }
            ]
          }
        },
        "build-storybook": {
          "builder": "@storybook/angular:build-storybook",
          "options": {
            "configDir": "projects/ui/.storybook",
            "browserTarget": "ui:build",
            "compodoc": false,
            "outputDir": "dist/storybook/ui",
            "styles":[
              "projects/ui/src/lib/global-styles.css"
            ],
            "assets": [

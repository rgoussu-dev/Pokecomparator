{
  "name": "pokecomparator",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "run:all": "node node_modules/@angular-architects/module-federation/src/server/mf-dev-server.js",
    "lint": "ng lint",
    "docs": "compodoc -p tsconfig.doc.json -d documentation --disableCoverage",
    "docs:serve": "compodoc -p tsconfig.doc.json -d documentation --disableCoverage -s",
    "docs:watch": "compodoc -p tsconfig.doc.json -d documentation --disableCoverage -s -w"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@10.9.0",
  "dependencies": {
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/router": "^21.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular-architects/module-federation": "^20.0.0",
    "@angular-devkit/architect": "^0.2100.0",
    "@angular-devkit/build-angular": "^21.0.1",
    "@angular-devkit/core": "^21.0.0",
    "@angular/build": "^21.0.1",
    "@angular/cli": "^21.0.1",
    "@angular/compiler-cli": "^21.0.0",
    "@angular/platform-browser-dynamic": "^21.0.0",
    "@compodoc/compodoc": "^1.1.32",
    "@storybook/addon-a11y": "^10.1.0",
    "@storybook/addon-docs": "^10.1.0",
    "@storybook/addon-onboarding": "^10.1.0",
    "@storybook/addon-styling-webpack": "^3.0.0",
    "@storybook/angular": "^10.1.0",
    "@storybook/preset-scss": "^1.0.3",
    "angular-eslint": "21.0.1",
    "css-loader": "^7.1.2",
    "eslint": "^9.39.1",
    "jsdom": "^27.1.0",
    "ng-packagr": "^21.0.0",
    "ngx-build-plus": "^20.0.0",
    "storybook": "^10.1.0",
    "style-loader": "^4.0.0",
    "typescript": "~5.9.2",
    "typescript-eslint": "8.46.4",
    "vitest": "^4.0.8"
  }
}